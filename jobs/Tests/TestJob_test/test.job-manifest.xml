<?xml version="1.0" ?>
<job-manifest enable="False">

    <outpath value="{OutputDir}"/>

    <execute command='python "{ResourcesDir}/simpleRender.py"' stage="stage1">
        <argument>--tool "{Tool}"</argument>
        <argument>--template "{ResourcesDir}/Templates/template_testjob_test.py"</argument>
        <argument>--render_mode "{RenderDevice}"</argument>
        <argument>--pass_limit {PassLimit}</argument>
        <argument>--resolution_x {rx}</argument>
        <argument>--resolution_y {ry}</argument>
        <argument>--res_path "{ScenesDir}"</argument>
        <argument>--output "{OutputDir}"</argument>
	    <argument>--package_name "TestJob_test"</argument>
        <argument>--test_list "{Root}/Tests/TestJob_test/{TestsFilter}TestsList.txt"</argument>
    </execute>

    <execute command='python "{ResourcesDir}/make_report_json.py"' stage="stage2">
        <argument>--work_dir "{OutputDir}"</argument>
    </execute>

    <execute command='python "{CommonDir}/scripts/HtmlTemplate/htmlBuilder.py"' stage="stage4">
        <argument>--work_dir "{OutputDir}"</argument>
    </execute>

</job-manifest>
